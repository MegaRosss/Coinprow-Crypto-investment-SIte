  <script>
    // const ws = new WebSocket("wss://stream.binance.com:9443/ws/btcusdt@ticker")


    function trendSetter( tickerSymbol, stockprice , stockpercentage, marketcap ){
      const socket = new WebSocket('wss://stream.binance.com:9443/ws')

// Subscribe to updates on the market cap of Bitcoin
socket.addEventListener('open', event => {
    socket.send(JSON.stringify({
        method: 'SUBSCRIBE',
        params: [tickerSymbol],
        id: 1
    }))
})
    const stockPrice = document.getElementById(stockprice)
    const stockPercentage = document.getElementById(stockpercentage)
    const marketCap = document.getElementById(marketcap)
    let lastPrice = NaN
    let lastPricePercentage = NaN

    function getColorForNumber(number, lastNumber) {
      const change = number - lastNumber
    if (change > 0) {
        return 'green'
    } else if (change < 0) {
        return 'red'
    } else {
        return 'black'
    }
}
    socket.onmessage = (event) =>{
      const data = JSON.parse(event.data)
      let price = data.c 
    let pricePercentage= data.P
    let priceCap= Math.floor(data.E)
  let priceData  = Math.floor(price * 1000)/1000
  
  
  let formattedPriceOptions = { style: 'currency', currency: 'USD' };
  let formattedPrice = priceData.toLocaleString('en-US', formattedPriceOptions);  
  let formattedPriceCap = priceCap.toLocaleString('en-US', formattedPriceOptions);  
  stockPrice.innerText = `USD ${formattedPrice}`
  stockPercentage.innerText = `${pricePercentage}%`
  marketCap.innerText = `${formattedPriceCap}`
  stockPrice.style.color = getColorForNumber(priceData, lastPrice)
  lastPrice = priceData
  stockPercentage.style.backgroundColor= getColorForNumber(pricePercentage, lastPricePercentage) == "black" ? "white" : getColorForNumber(pricePercentage, lastPricePercentage)
      // let price = parseFloat(data.c);
lastPricePercentage =pricePercentage
      // $("#price").text("$" + price.toFixed(2));
    };



    }
    trendSetter('btcusdt@ticker', "stock-price_btc","price-percentage_btc","market-cap_btc")
    trendSetter('ethusdt@ticker', "stock-price_eth","price-percentage_eth","market-cap_eth")
    trendSetter('busdusdt@ticker', "stock-price_busd","price-percentage_busd","market-cap_busd")
    trendSetter('bnbusdt@ticker', "stock-price_bnb","price-percentage_bnb","market-cap_bnb")
  </script>
